n = int(input('n = '))

bir = n % 10
on = n // 10 % 10
yuz = n // 100 % 10
ming = n // 1000 % 10
onming = n // 10000 % 10
yuzming = n // 100000 % 10
million = n // 1000000 % 10
onmillion = n // 10000000 % 10
yuzmillion = n // 100000000 % 10

s = str()

#Yuz million

if yuzmillion == 1:
    a = 'bir yuz'
elif yuzmillion == 2:
    a = 'ikki yuz'
elif yuzmillion == 3:
    a = 'uch yuz'
elif yuzmillion == 4:
    a = "to'rt yuz"
elif yuzmillion == 5:
    a = 'besh yuz'
elif yuzmillion == 6:
    a = 'olti yuz'
elif yuzmillion == 7:
    a = 'yetti yuz'
elif yuzmillion == 8:
    a = 'sakkiz yuz'
elif yuzmillion == 9:
    a = "to'qqiz yuz"

# On million    
    
if onmillion == 1:
    b = "o'n"
elif onmillion == 2:
    b = "yigirma"
elif onmillion == 3:
    b = "o'ttiz"
elif onmillion == 4:
    b = "qirq"
elif onmillion == 5:
    b = "ellik"
elif onmillion == 6:
    b = "oltmish"
elif onmillion == 7:
    b = "yetmish"
elif onmillion == 8:
    b = "sakson"
elif onmillion == 9:
    b = "to'qson"
    
# Million
    
if million == 1:
    c = "bir"
elif million == 2:
    c = "ikki"
elif million == 3:
    c = "uch"
elif million == 4:
    c = "to'rt"
elif million == 5:
   c = "besh"
elif million == 6:
    c = "olti"
elif million == 7:
    c = "yetti"
elif million == 8:
    c = "sakkiz"
elif million == 9:
    c = "to'qqiz"
    
if yuzmillion:
    s += a+" "
if onmillion:
    s += b+" "
if million:
    s += c+" "
if yuzmillion or onmillion or million:
    s += 'million '


# Yuz ming
    
if yuzming == 1:
