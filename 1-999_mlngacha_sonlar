elif onming == 7:
    e = "yetmish"
elif onming == 8:
    e = "sakson"
elif onming == 9:
    e = "to'qson"
    
#Ming
    
if ming == 1:
    f = "bir"
elif ming == 2:
    f = "ikki"
elif ming == 3:
    f = "uch"
elif ming == 4:
    f = "to'rt"
elif ming == 5:
    f = "besh"
elif ming == 6:
    f = "olti"
elif ming == 7:
    f = "yetti"
elif ming == 8:
    f = "sakkiz"
elif ming == 9:
    f = "to'qqiz"
    
if yuzming:
    s += d+" "
if onming:
    s += e+" "
if ming:
    s += f+" "
if yuzming or onming or ming:
    s += "ming "


# Yuz
    
